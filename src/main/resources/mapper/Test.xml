<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cf.eisp.exam_1.repository.TestRepository">
    <select id="findByRandomInt" resultType="ExamPaper" parameterType="Integer">
        select * from exam_paper where exam_paper_id = #{randomInt}
    </select>
</mapper>